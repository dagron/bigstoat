{
  "name": "bigstoat",
  "version": "0.1.0",
  "description": "Cross platform File Manager",
  "main": "src/electron/electron.js",
  "scripts": {
    "build": "npm run build:electron && npm run build:app",
    "build:app": "webpack --config webpack.config.app.js",
    "build:app:watch": "webpack --config webpack.config.app.js -w",
    "build:electron": "babel -d dist/electron --babelrc .babelrc_electron src/electron",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "run": "electron dist/electron/electron.js",
    "start": "npm run build:app && npm run build:electron && npm run run"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:francisl/bigstoat.git"
  },
  "keywords": [
    "File Manager",
    "File Explorer",
    "html5",
    "electron"
  ],
  "author": "Francis Lavoie",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/atom/electron-quick-start/issues"
  },
  "homepage": "https://github.com/atom/electron-quick-start#readme",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.26.0",
    "electron": "^1.4.7",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.35.0",
    "node-sass": "^3.4.2",
    "react-redux": "^4.4.2",
    "redux": "^3.4.0",
    "redux-thunk": "^2.0.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "semantic-ui": "^2.2.4",
    "semantic-ui-css": "^2.1.8",
    "url-loader": "^0.5.7"
  }
}
