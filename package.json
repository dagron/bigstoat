{
  "name": "bigstoat",
  "version": "0.1.0",
  "description": "Cross platform File Manager",
  "main": "src/electron.js",
  "scripts": {
    "build": "npm run build:electron; npm run build:app",
    "build:app": "webpack --config webpack.conf.app.js -w",
    "build:electron": "babel -o dist/electron.bundle.js --babelrc .babelrc_electron src/electron.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "start": "npm run flow; npm run build:electron; electron dist/electron.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:francisl/bigstoat.git"
  },
  "keywords": [
    "File Manager",
    "File Explorer",
    "html5",
    "electron"
  ],
  "author": "Francis Lavoie",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/atom/electron-quick-start/issues"
  },
  "homepage": "https://github.com/atom/electron-quick-start#readme",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "electron-prebuilt": "^0.37.2",
    "electron-rebuild": "^1.1.3",
    "flow-bin": "^0.22.1",
    "node-sass": "^3.4.2",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-mdl": "^1.5.2"
  }
}
